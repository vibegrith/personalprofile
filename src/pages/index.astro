	  </div>
	  <div class="flex flex-row gap-3">
      <Image src={Css} alt="Css logo"></Image>
	  <Image src={Html} alt="Html logo"></Image>
	  <Image src={Github} alt="Github logo"></Image>
	  <Image src={Tailwind} alt="Tailwind logo"></Image>
	  </div>
	  
    </div>
    

  </div>
</div>

</section>


</MainLayout>

<script>
	import { inView } from "motion";

	const inviewelement = document.querySelector(".section1");
	inView(
		inviewelement , () => {
			inviewelement.classList.add("animate__animated","animate__fadeInUp", "animate__delay-1s")
